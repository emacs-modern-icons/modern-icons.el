;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
;; Core function to create icons

(defun modern-icons-create-icon (icon-class icon-name &optional size)
  "Create an icon descriptor from ICON-CLASS and ICON-NAME for SIZE."
  (let* ((icon-dir (concat modern-icons-root-dir
                           (file-name-as-directory "icons")
                           (file-name-as-directory icon-class)))
         (background (frame-parameter nil 'background-mode))
         (size (or size modern-icons-icon-size))
         (icon-key (format "%s:%s:%s:%s" icon-class icon-name background size)))
    (or (gethash icon-key modern-icons-icon-cache)
        (let* ((icon-file-default (concat icon-dir icon-name))
               (icon-file-dark (concat icon-dir (file-name-as-directory "dark") icon-name))
               (icon-file-light (concat icon-dir (file-name-as-directory "light") icon-name))
               (icon-file (or (and (equal background 'dark) (file-exists-p icon-file-dark)
                                   icon-file-dark)
                              (and (equal background 'light) (file-exists-p icon-file-light)
                                   icon-file-light)
                              (and (file-exists-p icon-file-default)
                                   icon-file-default)))
               (icon (create-image icon-file 'svg nil :width size :height size :ascent 'center)))
          (puthash icon-key icon modern-icons-icon-cache)))))

;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
;; Get icons

(defun modern-icons-icon-for-file-name (file-name)
  "Create an icon for FILE-NAME by matching exact file name.
Return NIL if no icon is found."
  (when-let* ((file-name (downcase (file-name-nondirectory file-name)))
              (icon-data (cadr (assoc file-name modern-icons-file-name-icon-alist))))
    (modern-icons-create-icon (car icon-data) (cadr icon-data))))

(defun modern-icons-icon-for-file-ext (file-name)
  "Create an icon for FILE-NAME by checking file extension.
Return NIL if no icon is found."
  (when-let* ((file-name (downcase (file-name-nondirectory file-name)))
              (file-ext (if-let (ext (file-name-extension file-name)) ext ""))
              (icon-data (and (not (string= file-ext ""))
                              (cadr (assoc file-ext modern-icons-file-ext-icon-alist)))))
    (modern-icons-create-icon (car icon-data) (cadr icon-data))))

(defun modern-icons-icon-for-file (file-name)
  "Create an icon for FILE-NAME by checking both file name and extension.
Return NIL if no icon is found."
  (when-let* ((file-name (downcase (file-name-nondirectory file-name)))
              (file-ext (if-let (ext (file-name-extension file-name)) ext ""))
              (icon-data (or (cadr (assoc file-name modern-icons-file-name-icon-alist))
                             (and (not (string= file-ext ""))
                                  (cadr (assoc file-ext modern-icons-file-ext-icon-alist))))))
    (modern-icons-create-icon (car icon-data) (cadr icon-data))))

(defun modern-icons-icon-for-dir (dir-name)
  "Create an icon for DIR-NAME. Return NIL if no icon is found."
  (when-let* ((dir-name (downcase (file-name-nondirectory (directory-file-name dir-name))))
              (icon-data (cadr (assoc dir-name modern-icons-dir-icon-alist))))
    (modern-icons-create-icon (car icon-data) (cadr icon-data))))

(defun modern-icons-icon-for-major-mode (mode)
  "Create an icon for the major mode MODE. Return NIL if no icon is found."
  (when-let* ((mode (downcase (format "%s" mode)))
              (icon-data (cadr (assoc mode modern-icons-major-mode-icon-alist #'string=))))
    (modern-icons-create-icon (car icon-data) (cadr icon-data))))

(defun modern-icons-icon-for-minor-mode (mode)
