;;; Code:

(require 'cl-lib)

(defgroup modern-icons nil
  "Modern icons for Emacs."
  :prefix "modern-icons-"
  :group 'appearance
  :group 'convenience)

;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
;; Customization

(defcustom modern-icons-icon-size 18
  "Icon size in pixel to be displayed."
  :group 'modern-icons
  :type 'number)

;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
;; Variables

(defvar modern-icons-root-dir
  (or (and load-file-name (file-name-directory load-file-name))
      (when-let ((lib-path (locate-library "modern-icons")))
        (file-name-directory lib-path)))
  "Root directory of this `modern-icons' library where it is loaded.")

